{
	"name": "top24-nest",
	"version": "1.0.0",
	"description": "Backend service for cargo transportation app.",
	"author": "Alvin Ahmadov",
	"private": true,
	"license": "MIT licensed",
	"scripts": {
		"doc": "node node_modules/.bin/compodoc src",
		"build": "nest build",
		"migrate": "sequelize-cli db:migrate",
		"migrate:undo": "sequelize-cli db:migrate:undo",
		"start": "node dist/src/main.js",
		"start:dev": "nest start",
		"start:watch": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main.js",
		"parse:address": "node dist/lib/parse-tools/addressParser.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
